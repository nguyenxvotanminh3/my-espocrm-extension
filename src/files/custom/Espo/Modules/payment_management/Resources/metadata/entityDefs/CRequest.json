{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "requestId": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 5,
            "isCustom": true
        },
        "branch": {
            "type": "link"
        },
        "region": {
            "readOnly": true,
            "type": "foreign",
            "link": "branch",
            "field": "region",
            "view": "views/fields/foreign-varchar",
            "isCustom": true
        },
        "priority": {
            "type": "enum",
            "options": [
                "Thường",
                "Ưu tiên"
            ],
            "style": {
                "Thường": null,
                "Ưu tiên": null
            },
            "default": "Thường",
            "maxLength": 100,
            "isCustom": true
        },
        "paymentPeriod": {
            "type": "enum",
            "options": [
                "Tháng 1",
                "Tháng 2",
                "Tháng 3",
                "Tháng 4",
                "Tháng 5",
                "Tháng 6",
                "Tháng 7",
                "Tháng 8",
                "Tháng 9",
                "Tháng 10",
                "Tháng 111",
                "Tháng 12"
            ],
            "default": "Tháng 1",
            "style": {
                "Tháng 1": null,
                "Tháng 2": null,
                "Tháng 3": null,
                "Tháng 4": null,
                "Tháng 5": null,
                "Tháng 6": null,
                "Tháng 7": null,
                "Tháng 8": null,
                "Tháng 9": null,
                "Tháng 10": null,
                "Tháng 111": null,
                "Tháng 12": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "proposalType": {
            "type": "enum",
            "options": [
                "Tạm ứng",
                "Thanh Toán"
            ],
            "default": "Tạm ứng",
            "style": {
                "Tạm ứng": null,
                "Thanh Toán": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "amount": {
            "type": "currency",
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "branch": {
            "type": "belongsTo",
            "foreign": "requests",
            "entity": "CBranch",
            "audited": false,
            "isCustom": true
        },
        "requestAttachments": {
            "type": "hasMany",
            "foreign": "request",
            "entity": "CRequestAttachment",
            "audited": false,
            "isCustom": true
        },
        "requestCostItems": {
            "type": "hasMany",
            "relationName": "cRequest_costitem",
            "foreign": "requests",
            "entity": "CRequestCostItem",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}